<form action="" method="get">
  <div class="form-group row">
    <label for="id" class="col-sm-2 col-form-label">
      <input name="cid" type="checkbox" class="form-check-input" value="true" <%= query.cid ? ' checked' : ''%>>ID
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " id="id" name="id" value="<%= query.id %>">
  </div>
  </div>


  <div class="form-group row">
    <label for="nama" class="col-sm-2 col-form-label">
      <input  name="cnama" type="checkbox" class="form-check-input" value="true" <%= query.cnama ? ' checked' : ''%>>Nama
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id = "nama" name="nama" value="<%= query.nama %>">
  </div>
  </div>


  <div class="form-group row">
    <label for="umur" class="col-sm-2 col-form-label">
      <input name="cumur" type="checkbox" class="form-check-input" value="true" <%= query.cumur ? ' checked' : ''%>>Umur
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " id = "umur" name="umur" value="<%= query.umur %>">
  </div>
  </div>


  <div class="form-group row">
    <label for="tinggi" class="col-sm-2 col-form-label">
      <input name="ctinggi"  type="checkbox"  class="form-check-input" value="true" <%= query.ctinggi ? ' checked' : ''%>>Tinggi
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " id = "tinggi" name="tinggi" value="<%= query.tinggi %>">
  </div>
  </div>


  <div class="form-group row">
    <label for="tanggallahir" class="col-sm-2 col-form-label">
      <input name="ctanggallahir" type="checkbox"  class="form-check-input" value="true" <%= query.ctanggallahir ? ' checked' : '' %>>Tanggal Lahir
    </label>
    <div class="col-sm-10">
      <input type="date" class="form-control " id = "tanggallahir" name="startdate" value="<%= query.startdate %>"> to
      <input type="date" class="form-control " id = "tanggallahir" name="enddate" value="<%= query.enddate %>">
  </div>
  </div>


  <div class="form-group row">
    <label for="ismenikah" class="col-sm-2 col-form-label">
      <input name="cismenikah" type="checkbox" class="form-check-input" value="ismenikah" <%= query.ismenikah ? ' checked' : ''%>> Status
    </label>

    <div class="col-sm-10">
      <select class="form-control" id="status" name= "ismenikah">
        <option value="true" <%= query.ismenikah ? ' selected' : ''%>>Sudah Menikah</option>
        <option value="false" <%= query.ismenikah ? ' selected' : ''%>>Belum Menikah</option>
      </select>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-primary">Search</button>
      <a href="/" class="btn btn-warning">reset</a>
    </div>

  </div>


</form>
